{
  "version": 2,
  "builds": [
    {
        "src": "./main.py",
        "use": "@vercel/python",
        "config": {
        "includeFiles": [
          "funciones/**"
        ]
        }
    }
  ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/main.py"
        }
        
    ]
  }